                    </div>
                    <div class="mt-3">
                        <input type="text" name="password" id="password" class="form-control" novalidate placeholder="password">
                    </div>

                    <div class="btns">
                        <button class="btn btn-success mt-3">signIN</button>

                    </div>
                </form>
        </div>
    


    </main>

    
    <script src="/js/nav.js"></script>

